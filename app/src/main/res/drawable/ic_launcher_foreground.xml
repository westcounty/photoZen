<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!--
    PhotoZen Logo - 禅意取景框
    设计理念：将相框与禅意流水曲线结合
    - 外框：圆润取景框代表照片
    - 内部：优雅S曲线代表禅意的流动与平衡
    - 光效：微妙高光增添精致感
    -->

    <!-- 主取景框 - 带渐变效果的圆角矩形 -->
    <group>
        <!-- 外框描边 - 青绿渐变 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#5EEAD4"
            android:strokeWidth="3.5"
            android:strokeLineCap="round"
            android:strokeLineJoin="round"
            android:pathData="M32,30
                C32,26.686 34.686,24 38,24
                L70,24
                C73.314,24 76,26.686 76,30
                L76,78
                C76,81.314 73.314,84 70,84
                L38,84
                C34.686,84 32,81.314 32,78
                Z" />
    </group>

    <!-- 禅意曲线 - 优雅的流动S形 (代表和谐与平衡) -->
    <group>
        <!-- 主曲线 - 从左下流向右上 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#5EEAD4"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M40,68
                Q48,58 54,54
                Q60,50 68,40" />

        <!-- 辅助曲线 - 平行的细线，增加韵律感 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#5EEAD4"
            android:strokeWidth="2.5"
            android:strokeLineCap="round"
            android:strokeAlpha="0.6"
            android:pathData="M40,60
                Q47,52 54,48
                Q61,44 68,34" />
    </group>

    <!-- 精选标记 - 代表"筛选/确认"的圆点 -->
    <group>
        <!-- 核心圆点 - 象征精选的照片 -->
        <path
            android:fillColor="#5EEAD4"
            android:pathData="M54,54 m-6,0 a6,6 0 1,1 12,0 a6,6 0 1,1 -12,0" />

        <!-- 内部高光点 - 增加精致感 -->
        <path
            android:fillColor="#FFFFFF"
            android:fillAlpha="0.9"
            android:pathData="M51,51 m-2,0 a2,2 0 1,1 4,0 a2,2 0 1,1 -4,0" />
    </group>

    <!-- 装饰元素 - 角落的微妙点缀 -->
    <group>
        <!-- 左上角小圆点 - 取景器对焦点风格 -->
        <path
            android:fillColor="#5EEAD4"
            android:fillAlpha="0.4"
            android:pathData="M38,30 m-2,0 a2,2 0 1,1 4,0 a2,2 0 1,1 -4,0" />

        <!-- 右下角小圆点 -->
        <path
            android:fillColor="#5EEAD4"
            android:fillAlpha="0.4"
            android:pathData="M70,78 m-2,0 a2,2 0 1,1 4,0 a2,2 0 1,1 -4,0" />
    </group>

    <!-- 顶部光泽线 - 高级感 -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="1"
        android:strokeAlpha="0.15"
        android:strokeLineCap="round"
        android:pathData="M36,26 L72,26" />

</vector>
