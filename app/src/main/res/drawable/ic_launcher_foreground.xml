<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!--
    PhotoZen Logo - 禅意取景框
    设计理念：简洁的禅意流水曲线
    - 内部：优雅S曲线代表禅意的流动与平衡
    - 光效：微妙高光增添精致感
    -->

    <!-- 禅意曲线 - 优雅的流动S形 (代表和谐与平衡) -->
    <group>
        <!-- 主曲线 - 从左下流向右上 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#5EEAD4"
            android:strokeWidth="4"
            android:strokeLineCap="round"
            android:pathData="M40,68
                Q48,58 54,54
                Q60,50 68,40" />

        <!-- 辅助曲线 - 平行的细线，增加韵律感 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#5EEAD4"
            android:strokeWidth="2.5"
            android:strokeLineCap="round"
            android:strokeAlpha="0.6"
            android:pathData="M40,60
                Q47,52 54,48
                Q61,44 68,34" />
    </group>

    <!-- 精选标记 - 代表"筛选/确认"的圆点 -->
    <group>
        <!-- 核心圆点 - 象征精选的照片 -->
        <path
            android:fillColor="#5EEAD4"
            android:pathData="M54,54 m-6,0 a6,6 0 1,1 12,0 a6,6 0 1,1 -12,0" />

        <!-- 内部高光点 - 增加精致感 -->
        <path
            android:fillColor="#FFFFFF"
            android:fillAlpha="0.9"
            android:pathData="M51,51 m-2,0 a2,2 0 1,1 4,0 a2,2 0 1,1 -4,0" />
    </group>


</vector>
